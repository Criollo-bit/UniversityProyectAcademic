generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Career {
  id     Int    @id @default(autoincrement())
  name   String
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  email    String @unique
  password String
  role     String
}

model Cycle {
  id     Int    @id @default(autoincrement())
  name   String
}

model Enrollment {
  id             Int      @id @default(autoincrement())
  studentId      Int
  subjectId      Int
  academicPeriod String
  date           DateTime @default(now())
}